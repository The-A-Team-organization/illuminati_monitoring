{
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "type": "graph",
      "title": "CPU Usage",
      "targets": [
        {
          "expr": "rate(node_cpu_seconds_total{mode!=\"idle\"}[5m])",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Memory Usage",
      "targets": [
        {
          "expr": "node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes",
          "legendFormat": "{{instance}}"
        }
      ]
    }
  ],
  "schemaVersion": 16,
  "title": "Node Overview",
  "version": 1
}
